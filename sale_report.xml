<odoo>
    <data>

        <!-- Override Sale Order PDF Report -->
        <record id="sale.action_report_saleorder" model="ir.actions.report">
            <field name="name">Custom Sale Order</field>
            <field name="model">sale.order</field>
            <field name="report_type">qweb-pdf</field>

            <!-- Custom Template -->
            <field name="report_name">fsz_sale_report_override.custom_sale_report</field>
            <field name="report_file">fsz_sale_report_override.custom_sale_report</field>

            <!-- Filename -->
            <field name="print_report_name">'Sale Order - %s' % (object.name)</field>
        </record>

    </data>
</odoo>
